struct Node {}

predicate ls(x: Node)

procedure spatial_neq2(x1: Node, x2: Node, x3: Node, x4: Node, x5: Node,
                       x6: Node, x7: Node)
  requires acc(x1) &*& (ls(x7) && acc(x2))
  &*& ((acc(x3) &*& ls(x4)) && (ls(x6) &*& [acc(x5)](x5)))
  ensures acc(x1) &*& (ls(x7) && acc(x2))
  &*& ((acc(x3) &*& ls(x4)) && (ls(x6) &*& [acc(x5)](x5)))
{
  pure assert x3 != x5;
}
